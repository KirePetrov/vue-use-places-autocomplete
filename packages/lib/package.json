    "vue": "^3.0.11",
    "vue-composition-test-utils": "^1.0.3"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.1",
    "@vueuse/core": "^5.2.0",
    "vue-demi": "^0.11.2"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
