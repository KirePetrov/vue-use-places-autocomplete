  "license": "MIT",
  "main": "./dist/v-use-places-autocomplete.umd.js",
  "module": "./dist/v-use-places-autocomplete.es.js",
  "types": "./dist",
  "exports": {
    ".": {
      "import": "./dist/v-use-places-autocomplete.es.js",
      "require": "./dist/v-use-places-autocomplete.umd.js"
    }
  },
  "scripts": {
