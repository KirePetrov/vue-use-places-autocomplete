{
  "name": "vue-use-places-autocomplete",
  "version": "0.1.1",
  "packageManager": "pnpm@7.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wobsoriano/v-use-places-autocomplete.git"
  },
  "keywords": [
    "vue",
    "places",
    "autocomplete"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "nr build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest run"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.14.3",
    "@vueuse/core": "^8.7.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.16.2",
    "@types/google.maps": "^3.49.2",
    "@types/node": "^16.4.10",
    "bumpp": "^8.2.1",
    "eslint": "^8.18.0",
    "jsdom": "^20.0.0",
    "pnpm": "^7.3.0",
    "tsup": "^6.1.2",
    "typescript": "^4.3.2",
    "vitest": "^0.16.0",
    "vue": "^3.2.0",
    "vue-test-composables": "^0.1.5"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
